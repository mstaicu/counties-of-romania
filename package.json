{
  "name": "counties-of-romania",
  "version": "0.0.0-semantically-released",
  "description": "Romanian counties",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "romanian",
    "romania",
    "counties"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-cli": "6.24.1",
    "babel-loader": "7.1.1",
    "babel-plugin-istanbul": "4.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "codecov": "2.2.0",
    "commitizen": "2.9.6",
    "cross-env": "5.0.1",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.1.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.6.0",
    "husky": "0.14.1",
    "json-loader": "0.5.4",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.3-candidate.0",
    "rimraf": "2.6.1",
    "semantic-release": "6.3.6",
    "validate-commit-msg": "2.12.2",
    "webpack": "3.0.0"
  },
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prebuild": "rimraf dist",
    "build": "cross-env BABEL_ENV=production npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "lint": "eslint src",
    "report-coverage": "cat ./coverage/lcov.infp | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "cross-env BABEL_ENV=test nyc ava",
    "validate": "npm-run-all --parallel lint test build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mstaicu/counties-of-romania.git"
  },
  "author": "Mircea Staicu <mstaicu@yahoo.com>",
  "license": "MIT"
}
